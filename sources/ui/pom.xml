<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<parent>
		<artifactId>tools.dynamia.modules.email.parent</artifactId>
		<groupId>tools.dynamia.modules</groupId>
		<version>2.6.1</version>
	</parent>
	<modelVersion>4.0.0</modelVersion>
	<name>DynamiaModules - Email UI</name>
	<artifactId>tools.dynamia.modules.email.ui</artifactId>
	<version>2.6.1</version>
	<url>https://www.dynamiasoluciones.com</url>

	<dependencies>

		<dependency>
			<groupId>tools.dynamia</groupId>
			<artifactId>tools.dynamia.zk.crud</artifactId>
			<version>${dynamiatools.version}</version>
		</dependency>
		<dependency>
			<groupId>tools.dynamia.modules</groupId>
			<artifactId>tools.dynamia.modules.email</artifactId>
			<version>2.6.1</version>
		</dependency>
		<dependency>
			<groupId>tools.dynamia.zk.addons</groupId>
			<artifactId>aceditor-zk</artifactId>
			<version>1.4.4</version>
		</dependency>
	</dependencies>


	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>${maven.compiler}</version>
				<configuration>
					<source>${java.version}</source>
					<target>${java.version}</target>
					<encoding>${source.encoding}</encoding>
				</configuration>
			</plugin>
		</plugins>
	</build>
</project>
